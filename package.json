    "node-sass": "^3.4.2",
    "npm-run-all": "^1.7.0",
    "offline-plugin": "^2.0.4",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
