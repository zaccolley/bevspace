{
  "private": true,
  "name": "bevspace",
  "version": "0.0.1",
  "description": "final year project",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdir -p build && ncp src/assets build/assets",
    "test": "snyk test && npm run lint",
    "lint": "eslint src tests/**/*.js",
    "deploy": "npm run snyk-protect && npm run test && npm run build && surge ./build -d bev.space",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "license": "MIT",
  "author": "Zac Colley <hi@zaccolley.com>",
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "core-js": "^2.2.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "es6-promise": "^3.1.2",
    "eslint": "^2.5.3",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^0.2.2",
