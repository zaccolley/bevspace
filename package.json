{
  "private": true,
  "name": "bevspace",
  "version": "0.0.2",
  "description": "final year project",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress && ncp build/index.html build/200.html",
    "prebuild": "mkdir -p build && ncp src/service-worker.js build/service-worker.js && ncp src/manifest.json build/manifest.json && ncp src/assets build/assets",
    "test": "snyk test && npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}",
    "deploy": "npm run build && surge --project ./build/ --domain bevspace.surge.sh",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "license": "MIT",
  "author": "Zac Colley <hi@zaccolley.com>",
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel": "6.5.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-jsx": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-minimal": "2.1.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "babel-runtime": "6.22.0",
    "chai": "3.5.0",
    "core-js": "2.4.1",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "1.2.0",
    "es6-promise": "4.0.5",
    "eslint": "3.14.1",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.0",
    "json-loader": "0.5.4",
    "karma": "1.4.1",
    "karma-chai": "0.1.0",
    "karma-chai-sinon": "0.1.5",
    "karma-es6-shim": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mocha": "3.2.0",
    "moment": "2.17.1",
    "ncp": "2.0.0",
    "node-sass": "4.5.0",
    "phantomjs-prebuilt": "2.1.14",
    "postcss-loader": "1.2.2",
    "raw-loader": "0.5.1",
    "sass-loader": "4.1.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "snyk": "1.24.6",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "superstatic": "4.0.3",
    "surge": "0.18.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "1.16.3",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "brauhaus": "1.1.1",
    "decko": "1.1.3",
    "pouchdb": "6.1.1",
    "pouchdb-authentication": "0.5.5",
    "preact": "7.1.0",
    "preact-compat": "3.13.1",
    "preact-router": "2.4.1",
    "proptypes": "0.14.3",
    "wildemitter": "1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
